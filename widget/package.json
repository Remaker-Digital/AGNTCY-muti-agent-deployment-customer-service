{
  "name": "@agntcy/chat-widget",
  "version": "1.0.0",
  "description": "AGNTCY Embedded JavaScript Chat Widget for Customer Service",
  "main": "dist/agntcy-chat.min.js",
  "module": "dist/agntcy-chat.esm.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
